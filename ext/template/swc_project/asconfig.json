{
    "config": "flex",
    "type": "lib",
    "compilerOptions": {
        "source-path": [
            "src",
            @@external_src@@
        ],
        "include-sources": [
            "src",
        ],
        "output": "bin/@@compile_version@@/@@project_name@@.swc",
        "debug": @@is_debug@@,
        "load-config": [],
        "swf-version": @@swf_version@@,
        "target-player": "@@player_version@@",
        "static-link-runtime-shared-libraries": true,
        "library-path": [
            "@@global_lib_root@@",
            "@@global_lib@@",
            "@@local_lib_root@@",
            "@@local_lib@@",
            ".lib/@@compile_version@@",
        ],
        "external-library-path": [
            "@@global_rsl_root@@",
            "@@global_rsl@@",
            "@@local_rsl_root@@",
            "@@local_rsl@@",
            ".rsl/@@compile_version@@",
        ],
    },
}