<?xml version="1.0" encoding="UTF-8"?>
<project name="@@project_name@@" basedir=".">
	<property name="dir.bin" value="${basedir}/bin/${config.compile.version}" />
	<property name="dir.lib" value="${basedir}/lib/${config.compile.version}" />
	<property name="dir.rsl" value="${basedir}/rsl/${config.compile.version}" />
	<property name="dir.src" value="${basedir}/src" />

	<property name="dot.lib" value="${basedir}/.lib/${config.compile.version}" />
	<property name="dot.rsl" value="${basedir}/.rsl/${config.compile.version}" />

	<property name="in.file" value="${dir.src}/${ant.project.name}.as" />
	<property name="out.file" value="${dir.bin}/${ant.project.name}.swf" />

	<target name="custom">
		<!-- write custom code -->
	</target>

	<import file="../../swf_project.xml" />
</project>
